{
  "extends": ["react-app", "eslint:recommended", "plugin:react/recommended"],
  "plugins": ["react"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "react/prop-types": "off",
		"react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "no-unused-vars": "warn"
  }
}
